<UserControl x:Class="Views.LinearGraphView"
             xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:lvc="using:LiveChartsCore.SkiaSharpView.Avalonia"
             xmlns:vms="using:ViewModels"
             x:DataType="vms:LinearGraphViewModel">
  <UserControl.DataContext>
    <vms:LinearGraphViewModel/>
  </UserControl.DataContext>
    <Grid>
        <lvc:CartesianChart
            Series="{Binding Series}"
            XAxes="{Binding XAxes}"
            YAxes="{Binding YAxes}">
        </lvc:CartesianChart>
    </Grid>
</UserControl>